<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.EcommerceShop"
        tools:targetApi="31">
        <activity android:name=".MainActivity"
            android:exported="false">

        </activity>
        <activity
            android:name=".MainUserActivity"
            android:exported="false" />
        <activity
            android:name=".MainShopActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".qui.product_detail.ProductDetailActivity"
            android:exported="false" />
        <activity
            android:name=".qui.homeuser.HomeUserActivity"
            android:exported="false">

        </activity>
        <activity
            android:name=".nhan.Activity.UserOrdersActivity"
            android:exported="false"
            android:parentActivityName=".MainUserActivity">

        </activity>

    </application>

</manifest>